  "description": "This is a server that will be in charge of running the logic of the project for the dashboard that the client will see in their web-browser",
  "scripts": {
    "test": "jest --config jest.config.js",
    "build": "tsc -p ./tsconfig.json",
    "build:watch": "tsc -p ./tsconfig.json --watch",
    "build-test": "tsc -p ./tsconfig.test.json",
    "build-test:watch": "tsc -p ./tsconfig.test.json --watch",
    "start": "node dist/index.js",
    "dev": "nodemon -r ts-node/register src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hanra-s-work/area-rattrapage.git"
  },
