  "bugs": {
    "url": "https://github.com/Hanra-s-work/area-rattrapage/issues"
  },
  "homepage": "https://github.com/Hanra-s-work/area-rattrapage#readme",
  "dependencies": {
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.0",
    "@types/uuid4": "^2.0.3",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
